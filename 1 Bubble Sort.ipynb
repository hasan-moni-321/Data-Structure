   "source": [
    "#bubble sort\n",
    "def sort_func(l):\n",
    "    n = len(l)\n",
    "    \n",
    "    for i in range(n):  \n",
    "        for j in range(0, n-1):\n",
    "            if l[j] > l[j+1]:\n",
    "                l[j], l[j+1] = l[j+1], l[j]\n",
    "                      "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
